{
  "name": "fykosak/web-fol",
  "keywords": [
    "nette"
  ],
  "type": "project",
  "license": [
    "MIT",
    "BSD-3-Clause",
    "GPL-2.0",
    "GPL-3.0"
  ],
  "require": {
    "php": ">= 7.4",
    "nette/nette": "^v3.1.0",
    "fykosak/nette-fksdb-downloader": "dev-master#9c5e491",
    "fykosak/nette-fks-utils": "dev-dev-context-translator",
    "ext-soap": "*",
    "ext-dom": "*",
    "ext-gettext": "*",
    "h4kuna/assets": "^1.1",
    "ext-json": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.6.0",
    "nette/tester": "^v2.4.1",
    "phpstan/phpstan": "1.10.21"
  },
  "scripts": {
    "testPSR": "php -e vendor/squizlabs/php_codesniffer/bin/phpcs --standard=PSR12 app/",
    "testPHPStan": "vendor/bin/phpstan analyse app",
    "test": "vendor/bin/tester tests/ -p php -s -c tests/php.ini -j 8"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  }
}
